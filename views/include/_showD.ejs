<script>
    function showMore(j) {
        if(document.getElementById("row+"+j).style.display=="contents")
            document.getElementById("row+"+j).style.display="none";
        else
            document.getElementById("row+"+j).style.display="contents";
    }
    function theNote(note) {
        if(!note=="")
            window.alert(note);
    }
 </script>
<% for (var i=0; i<d.length; i++) { %>

<tr>
    <th><b><%=d[i].fName%></b></th>
    <th><b><%=d[i].lName%></b></th>
    <th><%=d[i].status%> <a id="notes+<%=i%>"> <div id="<%=d[i].notes%>" onclick="theNote(this.id)" class="fa-solid fa-envelope"></div></a></th>
    <th><%=d[i].numOfF%> נשים <%=d[i].numOfM%> גברים</th>
    <th><%=d[i].PhoneNumber%></th>

</tr>
<tr style="display: none; color:rgb(255, 96, 117);" id="row+<%=i%>">
    <!--<th style="color:rgb(255, 96, 117);"><%=d[i].email%> <br><%=d[i].PhoneNumber%></th>-->
    <th style="color:rgb(255, 96, 117);"><%=d[i].PhoneNumber%></th>
    <th style="color:rgb(134, 36, 97);"><%=d[i].numOfF%> נשים <%=d[i].numOfM%> גברים</th>
    <th style="color:rgb(255, 96, 117);"><a id="notes+<%=i%>"> <div id="<%=d[i].notes%>" onclick="theNote(this.id)" class="fa-solid fa-envelope"></div></a></th>
</tr>
<% } %>