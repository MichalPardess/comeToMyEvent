<script>
    function showMore(j) {
        if(document.getElementById("row+"+j).style.display=="contents")
            document.getElementById("row+"+j).style.display="none";
        else
            document.getElementById("row+"+j).style.display="contents";
    }
    function theNote(note) {
        if(!note=="")
            window.alert(note);
    }
 </script>
<% for (var i=0; i<e.length; i++) { %>

<tr>
    <th><%=e[i].name%></th>
    <th><%=e[i].status%></th>
    <th>
        <a> <div id="<%=i%>" onclick="showMore(this.id)" class="fa-solid fa-user"></div></a>
    </th>
</tr>
<tr style="display: none; color:var(--dark);" id="row+<%=i%>">
    <!--<th style="color:rgb(255, 96, 117);"><%=e[i].email%> <br><%=e[i].PhoneNumber%></th>-->
    <th style="color:var(--dark);"><%=e[i].PhoneNumber%></th>
    <th style="color:var(--dark);"><%=e[i].numOfF%> נשים <%=e[i].numOfM%> גברים</th>
    <th style="color:var(--dark);"><a id="notes+<%=i%>"> <div id="<%=e[i].notes%>" onclick="theNote(this.id)" class="fa-solid fa-envelope"></div></a></th>
</tr>
<% } %>