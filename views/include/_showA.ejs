<script>
    function showMore(j) {
        if(document.getElementById("row+"+j).style.display=="contents")
            document.getElementById("row+"+j).style.display="none";
        else{
            document.getElementById("row+"+j).style.display="contents";
        }
    
            
    }
    function theNote(note) {
        if(!note=="")
            window.alert(note);
    }
 </script>
<% for (var i=0; i<participents.length; i++) { %>

<tr>
    <th><%=participents[i].name%></th>
    <th><%=participents[i].status%></th>
    <th>
        <a> <div id="<%=i%>" onclick="showMore(this.id)" class="fa-solid fa-user"></div></a>
    </th>
</tr>
<tr style="display: none; color: rgb(0, 0, 0);" id="row+<%=i%>">
    <th  style="color: rgb(0, 0, 0);"><%=participents[i].PhoneNumber%></th>
    <th style="color: rgb(0, 0, 0);"><%=participents[i].numOfM%> גברים <%=participents[i].numOfF%> נשים <%=participents[i].numOfC%> ילדים</th>
    <th style="color: rgb(0, 0, 0);"><a id="notes+<%=i%>"> <div id="<%=participents[i].notes%>" onclick="theNote(this.id)" class="fa-solid fa-envelope"></div></a></th>
</tr>
<% } %>